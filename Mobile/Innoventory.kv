<Login>:
    name: "login"

    usernameIn: usernameIn
    passwordIn: passwordIn

    FloatLay:
        Img:
            source:"Inno.png"
            size_hint: 0.33,0.33
            pos_hint: {"center_x":0.5,"top":0.99}
            
        TextInput:
            id: usernameIn
            hint_text:"Username"
            font_size: (20) 
            pos_hint: {"center_x": 0.5 , "top":0.65}
            size_hint: 0.5, 0.07
            multiline: False
            
        TextInput:
            id: passwordIn
            hint_text:"Password"
            font_size: (20)
            pos_hint: {"center_x": 0.5, "top":0.55}
            size_hint: 0.5, 0.07
            multiline: False
            password: True
            
        Button:
            text: "Login"
            font_size: (20)
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.2

            on_release:
                root.login()
                root.manager.transition.direction = "left"

        Button:
            text: "Create Account"
            font_size: (12)
            pos_hint:{"x":0.3,"y":0.3}
            size_hint: 0.4, 0.1

            on_release:
                root.manager.transition.direction = "left"
                root.createAcct()





