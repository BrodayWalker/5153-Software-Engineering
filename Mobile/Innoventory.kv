<Homepage>:
    
    name: "homepage"

    searchPhraseIn: searchPhrase
    
    FloatLay:
        Img:
            source:"Inno.png"
            size_hint: 0.33,0.33
            pos_hint: {"center_x":0.5,"top":0.99}

        TextInput:
            id: searchPhrase
            hint_text: "Enter an item to search for"
            font_size: 20
            pos_hint: {"x": 0.05, "top":0.63}
            size_hint: .90, 0.05
            multiline: False

        Button:
            text: "Search"
            font_size: (20)
            pos_hint:{"center_x":0.5,"top":0.45}
            size_hint: 0.4, 0.2
 
            on_release:
                root.search()
                root.manager.transition.direction = "left"
                
        Button:
            text: "Logout"
            font_size: (20)
            pos_hint:{"x":0.05,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.manager.transition.direction = "right"
                root.logout()
        
        Button:
            text: "Settings"
            font_size: (20)
            pos_hint:{"right":0.95,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.manager.transition.direction = "down"
                root.settingsMenu()

        Button:
            text:"Cart"
            font_size: (20)
            pos_hint:{"center_x":0.5,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.manager.transition.direction = "down"
                root.manager.current = "shoppingCart"

                    

