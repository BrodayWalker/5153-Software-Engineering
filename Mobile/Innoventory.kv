<SettingsMenu>:
    name:"settingsMenu"

    FloatLay:

        Img:
            source:"Inno.png"
            size_hint: 0.33,0.33
            pos_hint: {"center_x":0.5,"top":0.99}

        Button:
            text: "Change\nPassword"
            halign: "center"
            valigh: "center"
            font_size: (20)
            pos_hint:{"center_x":0.5,"center_y":0.6}
            size_hint: 0.3, 0.1

            on_release:
                root.manager.transition.direction = "left"
                root.changePassword()

        Button:
            text: "Back"
            font_size: (20)
            pos_hint:{"x":0.05,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.manager.transition.direction = "up"
                root.manager.current = "homepage"

        Button:
            text:"Cart"
            font_size: (20)
            pos_hint:{"center_x":0.5,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.manager.transition.direction = "down"
                root.manager.current = "shoppingCart"


