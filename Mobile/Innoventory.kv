    qtyBuy: qtyBuy
    FloatLay:

        Label: 
            text: 'Product Info'
            size_hint: 0.2, 0.05
            pos_hint: {"center_x":0.5,"top":0.98}    
            font_size: (30)  

        Label: 
            text: 'Product ID'
            size_hint: 0.3, 0.11
            pos_hint: {"x":0.05,"top":0.915}    
            font_size: (20)  

        Label: 
            text: 'Name'
            size_hint: 0.3, 0.13
            pos_hint: {"x":0.05,"top":0.805}    
            font_size: (20)  

        Label: 
            text: 'Qty in stock'
            size_hint: 0.3, 0.13
            pos_hint: {"x":0.05,"top":0.695}    
            font_size: (20)  

        Label: 
            text: 'Available'
            size_hint: 0.3, 0.13
            pos_hint: {"x":0.05,"top":0.585}    
            font_size: (20)  

        Label:
            text: 'Price'
            size_hint: 0.3, 0.13
