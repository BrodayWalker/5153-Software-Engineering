<FloatLay>:
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size
            source:"background.jpg"

<GridLay>:
    canvas:
        Color:
            rgba: 0.3843, 0.7019, 0.8745, 1
        Rectangle:
            pos: self.pos
            size: self.size

<Img>:
    allow_stretch: True
    keep_ratio: True


