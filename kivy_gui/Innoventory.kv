<ChangePassword>:
    name:"changePassword"
    oldPasswordIn: oldPasswordIn
    newPasswordIn: newPasswordIn
    newPasswordIn2: newPasswordIn2
    FloatLay:

        Img:
            source:"Inno.png"
            size_hint: 0.33,0.33
            pos_hint: {"center_x":0.5,"top":0.99}

        TextInput:
            id: oldPasswordIn
            hint_text:"Enter your old password"
            font_size: (20) 
            pos_hint: {"center_x": 0.5 , "top":0.66}
            size_hint: 0.5, 0.07
            multiline: False
            password:True

        TextInput:
            id: newPasswordIn
            hint_text:"Enter your new password"
            font_size: (20) 
            pos_hint: {"center_x": 0.5 , "top":0.56}
            size_hint: 0.5, 0.07
            multiline: False
            password:True

        TextInput:
            id: newPasswordIn2
            hint_text:"Re-enter your new password"
            font_size: (20) 
            pos_hint: {"center_x": 0.5 , "top":0.46}
            size_hint: 0.5, 0.07
            multiline: False
            password:True            

        Button:
            text: "Back"
            font_size: (20)
            pos_hint:{"x":0.05,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.manager.transition.direction = "right"
                root.manager.current = "settingsMenu"
        
        Button:
            text: "Submit"
            font_size: (20)
            pos_hint:{"x":0.75,"y":0.05}
            size_hint: 0.2, 0.1

            on_release:
                root.changePass()
                root.manager.transition.direction = "left"
                root.manager.current = "homepage"

                

